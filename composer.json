{
  "name": "bfitech/zapstore",
  "description": "A simple SQL connection wrapper.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rendra Suroso",
      "email": "rs@cogsci.bandungfe.net"
    }
  ],
  "require": {
    "php": ">=5.5 || ~7.0",
    "bfitech/zapcore": "~1.3.0"
  },
  "require-dev": {
    "ext-sqlite3": "*",
    "ext-pdo_sqlite": "*",
    "ext-mysqlnd": "*",
    "ext-pdo_mysql": "*",
    "ext-pgsql": "*",
    "ext-pdo_pgsql": "*",
    "phpunit/phpunit": "~4.8"
  },
  "suggest": {
    "bfitech/zapstore-sqlite3": "SQLite3-specific support.",
    "bfitech/zapstore-mysql": "MySQL-specific support.",
    "bfitech/zapstore-pgsql": "PostgreSQL-specific support."
  },
  "autoload": {
    "psr-4": {
      "BFITech\\ZapStore\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.3.x-dev"
    }
  }
}
